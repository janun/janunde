{
  "name": "janunde",
  "version": "0.0.1",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build:postcss": "postcss core/static_src/styles/tailwind.css -o core/static/core/css/tailwind.css",
    "build:js": "cat core/static_src/core/js/*.js > core/static/core/js/app.js",
    "build:images": "cp -r core/static_src/core/images/* core/static/core/images",
    "build": "npm run build:js && npm run build:images && npm run build:postcss"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "dependencies": {
    "leaflet": "^1.6.0",
    "leaflet-modal": "^0.2.0",
    "leaflet.fullscreen": "^1.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.1.0",
    "eslint": "^6.8.0",
    "postcss": "^8.2.2",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "tailwindcss": "^2.0.2",
    "tailwindcss-animatecss": "^0.3.3",
    "tailwindcss-animations": "^2.0.0",
    "@tailwindcss/forms": "^0.2.1"
  }
}
